package com.company;

public class Outer {
    void display()
    {
        System.out.println("Inside outer class");
    }
    class inner
    {
        void display()
        {
            System.out.println("Inside inner class");
        }
    }

    public static void main(String[] args) {
        Outer obj=new Outer();
        obj.display();

        Outer.inner obj1=obj.new inner();
        obj1.display();

    }
}
